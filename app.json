{
  "expo": {
    "name": "Bela Mulher",
    "slug": "bela-mulher",
    "version": "1.0.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/png/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/png/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1B2A3E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "bela.mulher"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/png/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "bela.mulher"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
